#Read csv file in 
#Need something generic to take the place of testFile
imageJData <- read.csv(file="testFile.csv",head=TRUE,sep=",")

#Print out file
imageJData

#dimensions of data
dim(imageJData)

#print out first 6 rows of data
head(imageJData)

#print out last 6 rows of data
tail(imageJData)

#min,median,mean,max, count of data
summary(imageJData)

#Prints out column names
names(imageJData)

#Create a barplot comparing color
counts <- table(imageJData$Color)
barplot(counts, main="Color Distribution", 
        xlab="Color")

#number of columns
ncol(imageJData)

#Grabs all data rows that are complete
mysample <- imageJData[sample(1:nrow(imageJData), NROW(na.omit(imageJData)),
                          replace=FALSE),]

#Google Motion Chart Example
library(googleVis)
Motion=gvisMotionChart(mysample, 
                       idvar="Image", 
                       timevar="Width")
plot(Motion)

